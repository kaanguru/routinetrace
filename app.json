{
  "expo": {
    "name": "routinetrace",
    "slug": "routinetrace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/adaptive-icon.png",
    "scheme": "routinetrace",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kaanguru.routinetrace"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#fffaec"
      },
      "package": "com.kaanguru.routinetrace",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "dark": {
            "image": "./assets/splash-d.png",
            "backgroundColor": "#00173D"
          },
          "resizeMode": "contain",
          "backgroundColor": "#fffaec",
          "imageWidth": 720,
          "imageHeight": 1280
        }
      ],
      "expo-font",
      "expo-asset",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "routinetrace",
          "organization": "cemkaanguru"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "47190c62-542a-4702-b7ff-27df9781ba6f"
      }
    }
  }
}
